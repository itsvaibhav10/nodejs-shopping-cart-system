<!DOCTYPE html>
<html>

<head>
    <title>{{title}}</title>
    <link rel="shortcut icon" href="/images/apple.jpeg" type="image/x-icon">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.2.0/css/all.css">
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="/javascripts/jquery.js"></script>
    <link href="https://fonts.googleapis.com/css?family=Rammetto+One" rel="stylesheet">
    <!--Import Google Icon Font-->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Import materialize.css-->
    <link rel="stylesheet" href="/stylesheets/materialize.css">
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>

<body>
    {{>header}}
    {{{body}}}
    <!--JavaScript at end of body for optimized loading-->
    <script src="/javascripts/materialize.js"></script>
    <script>
        // Sidenav
        const sideNav = document.querySelector('.sidenav');
        M.Sidenav.init(sideNav, {});

        //Dropdown
        var dp = document.querySelectorAll('.dropdown-trigger');
        M.Dropdown.init(dp, {
            constrainWidth: false,
        });
        //Tooltip 
        const tooltip = document.querySelectorAll('.tooltipped');
        M.Tooltip.init(tooltip, {});

    </script>
</body>

</html>